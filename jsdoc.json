{
    "templates": {
        "referenceTitle": "ImapFlow",
        "disableSort": false,
        "collapse": true,
        "resources": {
            "Source Code": "https://github.com/nodemailer/imapflow"
        },
        "cleverLinks": true,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": false
        }
    },
    "plugins": ["plugins/markdown"],
    "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "private": true,
        "recurse": true,
        "template": "./node_modules/braintree-jsdoc-template"
    }
}
